<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="oper_db" class="com.kjt.service.common.datasource.DynamicDataSource">
		<property name="prefix" value="OPER_DB" />
	</bean>

	<bean id="oper_dbSlave" class="com.kjt.service.common.datasource.DynamicDataSource">
		<property name="prefix" value="OPER_DB_SLAVE" />
	</bean>

	<bean id="oper_dbMapQuery" class="com.kjt.service.common.datasource.DynamicDataSource">
		<property name="prefix" value="OPER_DB_MAPQUERY" />
	</bean>
</beans>

